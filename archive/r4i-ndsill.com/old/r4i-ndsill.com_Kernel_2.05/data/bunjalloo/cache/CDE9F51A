var ad_Text = new NTES.ui.Slide(NTES("#cRn1 span.con"), NTES("#cRn1 div.content"), "current", "mouseover", 5000),
    lastAdText = parseInt(NTES.cookie.get("NTES_adText")),
    curAdText;
if (isNaN(lastAdText)) {
    curAdText = 0;
} else {
    curAdText = lastAdText + 1;
    if (curAdText >= NTES("#cRn1 span.con").length) {
        curAdText = 0;
    }
    ad_Text.show(curAdText);
}
