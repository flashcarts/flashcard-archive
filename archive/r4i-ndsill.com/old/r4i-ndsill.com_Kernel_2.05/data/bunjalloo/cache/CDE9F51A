<script type="text/javascript">
var acIcon = document.getElementById("acIcon"); 
var ua = navigator.userAgent.toLowerCase();

function showNewUi() {
  acIcon.style.display = "block";
  var changeSkin = document.getElementById("changeSkin");
  changeSkin.style.top = "61px";
  var hotIcon = document.getElementById("hotIcon");
  hotIcon.style.display="none";
  var ydLogo = document.getElementById("ydLogo");
  ydLogo.style.top="30px";
  var ydTabs = document.getElementById("ydTabs");
  ydTabs.style.height="16px";
  ydTabs.style.marginRight="79px";

  var ydfContainer = document.getElementById("ydfContainer");
  ydfContainer.style.height="27px";
  ydfContainer.style.overflow="hidden";

  var ydHotKeys = document.getElementById("ydHotKeys");
  ydHotKeys.style.display="block";

  var ydQuery = document.getElementById("ydQuery");
  ydQuery.style.width="410px";

  var query = document.getElementById("query");
  query.style.height="17px";
  query.style.margin="-1px 0 0";
  query.style.padding="6px 5px 4px";
  query.value='预见未来的搜索';

  var ydSubmit = document.getElementById("ydSubmit");
  ydSubmit.style.margin="0";
  if (window.ActiveXObject && ua.match(/msie ([\d.]+)/)[1] == "6.0") {
  	ydSubmit.style.marginLeft="-3px";
  }
  ydSubmit.style.height="27px";
  ydSubmit.style.padding="27px 0 0 0";

  var ydLeft = document.getElementById("ydLeft");
  ydLeft.style.top="24px";

  var ydRight = document.getElementById("ydRight");
  ydRight.style.top="17px";
}


if (getUserType() != 0){
  showNewUi();
}

var hotwordbox = document.getElementById("hwbox");
var queryEl = document.getElementById("query");
var loop = true;
var oldui = (getUserType()==0);
function showAllHotWords(event){
if (oldui) {
    yclicklog(null, 'show_all_hotwords', '163_HOME_CLICK');
    loop = false;
    if (!noUserInput && queryEl.value.length > 0) {
      return;
    }
    var inputbox = document.getElementById("ydQuery");
    var position = SP.cumOffset(inputbox);
    var hotwordbox = document.getElementById("hwbox");
    hotwordbox.style.top = position[1] + (inputbox.offsetHeight + 2) + "px";
    if ((window.ActiveXObject && ua.match(/msie ([\d.]+)/)[1] == "8.0") || (navigator.userAgent.indexOf('Trident') >0) ) {
      hotwordbox.style.left = (position[0] - 2) + "px";
    } else {
      hotwordbox.style.left = (position[0] - 1) + "px";
    }

    if (window.ActiveXObject && ua.match(/msie ([\d.]+)/)[1] == "6.0") {
      hotwordbox.style.left = (position[0] - 1) + "px";
    }
    hotwordbox.style.cursor = "default";
    if (hotwordbox.style.display=="block"){
      hotwordbox.style.display="none";
      return;
    } 
    hotwordbox.style.width = (inputbox.offsetWidth + 2) + "px";
    hotwordbox.style.display = "block";
    if(typeof(event) != "undefined") {stopPropagation(event);}
}
}

function closeHotWords(event) {
if (oldui){
    hotwordbox.style.display="none";
    if (document.getElementById("query").value=='') {
      if (document.activeElement.id!="query") {
        setTimeout(function(){document.getElementById("query").className="yd-defaultquery"; document.getElementById("query").value=getRandomYdDefault();showHotIcon();}, 225);
      }
    }
}
}

function stopPropagation(e){
    e = e.browserEvent || e;
    if(e.stopPropagation){
        e.stopPropagation();
    } else {
        e.cancelBubble = true;
    }
} 
var hotIconEl=document.getElementById("hotIcon");
function clearHotIcon() {
  if (oldui){
    loop = false;
    hotIconEl.style.display = "none";
    acIcon.style.display = "block";
    noUserInput=false;
  }
}

function showHotIcon() {
    if(document.activeElement.id!="query") {
    loop = true;
    hotIconEl.style.display="block";
    acIcon.style.display = "none";
    noUserInput=true;
    }
}

function changeHotWord(url) {
  if (oldui){
    queryEl.className="yd-defaultquery"; 
    queryEl.value=getRandomYdDefault();
    showHotIcon();
    window.open(url,"_blank");
  } else {
    window.open('http://www.youdao.com/?keyfrom=163.newindex',"_blank");
  }
}
if (oldui){
setInterval(function(){if(loop&&document.activeElement.id!="query"){queryEl.value=getNextYdDefault();}},8000);
}
function isYdDefault(s) {return noUserInput; }

var nextIdx = 0;
function getNextYdDefault() {var len = ydDefaults.length;if (nextIdx >= len) { nextIdx = 0;} var res = ydDefaults[nextIdx];nextIdx++; return res; }
function getRandomYdDefault() {var len = ydDefaults.length; return ydDefaults[Math.floor(Math.random()*100)%len];}
if (oldui) {
document.getElementById("query").value = getRandomYdDefault();
}
function ydInputFocus(e,pos){
    if (oldui) {
	if(isYdDefault(e.value)) {
		e.className="yd-query";e.value='';
		clearHotIcon();
		return true;
	} 
    } else {
	if (e.className=='yd-defaultquery') {
		e.className="yd-query";e.value='';
		noUserInput=false;
	}
    }
	return false;
}

function ydInputBlur(e){
  if (!oldui) {
    if(e.value=='')  {
      if (document.activeElement.id!="query") {
        setTimeout(function(){e.className="yd-defaultquery";e.value='预见未来的搜索';noUserInput=true;}, 225);
      }
    }
  }
}

function getSearchUrl(inputId, product, keyfrom) {
   var url = "http://" + product + ".youdao.com/";
   if (window.RegExp && window.encodeURIComponent) {
     var input = document.getElementById(inputId);
     var query = input.value;
     if (isYdDefault(query)) query="";
     if (query != "") {
       query = query.replace(/(^link:)|(^inlink:)|(^related:)/,"");
       url = url+"search?q="+encodeURIComponent(query)+"&keyfrom="+keyfrom;
     } else {
       url = url+"?keyfrom="+keyfrom;
     }
   }
   return url;
}
function changeProduct(p) {
   var url = getSearchUrl("query", p, "163.index");
   window.open(url, "_blank");
   return false;
}
function clickYdSubmit() {
   var input = document.getElementById("query");
   var query = input.value;
   if (!isYdDefault(query)) {
     document.getElementById("ydForm").submit();
   } else {
	if (oldui){
	     changeHotWord("http://www.youdao.com/search?q="+query+"&keyfrom=163.index.indexhot");
	} else {
	     changeHotWord("http://www.youdao.com/search?q="+query+"&keyfrom=163.newindex");
	}
   }
   return false;
}
