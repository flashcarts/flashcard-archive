function genHotWordInNewUi() {
	if (showhot1) {document.write("<div id='hotkey1' style='display:block'>"); } 
	else {document.write("<div id='hotkey1' style='display:none'>");}
        var idx1 = [0,1,6,7,9];
        var idx2 = [2,3,4,5,8];
        sort(ydDefaults);
        if (ydDefaults.length == 10) {
                for (var i=0;i<idx1.length;i++){
                        if (i>0) {document.write('&nbsp;&nbsp;');}
                        var idx = idx1[i];
                        document.write('<a href="http://www.youdao.com/search?q='+ydDefaults[idx]+'&keyfrom=163.newindex.hotword" target="_blank">'+ydDefaults[idx]+'</a>');
                }
		if (showhot1) {document.write('</div><div id="hotkey2" style="display:none">');}
		else {document.write('</div><div id="hotkey2" style="display:block">');}
                for (var i=0;i<idx2.length;i++){
                        if (i>0) {document.write('&nbsp;&nbsp;');}
                        var idx = idx2[i];
                        document.write('<a href="http://www.youdao.com/search?q='+ydDefaults[idx]+'&keyfrom=163.newindex.hotword" target="_blank">'+ydDefaults[idx]+'</a>');
                }
                document.write('</div>');
                changeid = setInterval(changeWordNewUi,8000);
        } else {
                for (var i=0;i<5;i++){
                        if (i>0) {document.write('&nbsp;&nbsp;');}
                        var idx = i;
                        document.write('<a href="http://www.youdao.com/search?q='+ydDefaults[idx]+'&keyfrom=163.newindex.hotword" target="_blank">'+ydDefaults[idx]+'</a>');
                }
        }

}

function stopChangeWord(){
	clearInterval(changeid);
}

function startChangeWord(){
	changeid = setInterval(changeWordNewUi,8000);
}

function changeWordNewUi() {
        var hotkey1 = document.getElementById("hotkey1");
        var hotkey2 = document.getElementById("hotkey2");
        if (showhot1){
        hotkey1.style.display="none";hotkey2.style.display="block";showhot1=false;
        }else {
        hotkey2.style.display="none";hotkey1.style.display="block";showhot1=true;
        }
}

