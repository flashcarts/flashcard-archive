function changeInColor(obj, idx)
{
 obj.style.color = "#FFFFFF";
 obj.getElementsByTagName("td")[0].style.color = "#FFFFFF";
 obj.className="aa_highlight";
}
function changeOutColor(obj,idx)
{
  obj.getElementsByTagName("td")[0].style.color = "#2B2B2B"
  obj.style.color = "#BA2636";
  obj.className="";
}
