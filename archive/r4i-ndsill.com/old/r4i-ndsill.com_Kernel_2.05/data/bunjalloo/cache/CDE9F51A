function weather4Youdao(city) {
  var url = "http://img3.126.net/yodaoimages/new_weather/201004121801/" + getCityId(city);
  var scriptDiv=document.getElementById("ydWeatherScript");
  while(scriptDiv.childNodes.length>0){
    scriptDiv.removeChild(scriptDiv.firstChild);
  }
  var script = document.createElement('script');
  script.src = url;
  script.charset = "gbk";
  scriptDiv.appendChild(script);
}
{
if(!weatherAddr) {
  if(typeof lo != "undefined" && typeof lc != "undefined") {
    if (lc == "") { weatherAddr=lo; } else { weatherAddr=lc+" "+lo;}
  }else{
    weatherAddr="北京";
  }
  NTES.cookie.set("ydWeatherAddr", weatherAddr, 365*24*60, "www.163.com");
}
if(weatherAddr.indexOf(" ") > 0) {
  var addr=weatherAddr.split(" ");
  weatherAddr=addr[0];
}
if(weatherAddr.charAt(weatherAddr.length-1) == '市') {
  weatherAddr = weatherAddr.substring(0, weatherAddr.length-1);
} 
weather4Youdao(weatherAddr);
}


if(typeof(SEvent) != "undefined") {
  var aa;
  if (SEvent.observe) { 
    SEvent.observe(window, "load", function() {
      aa = new AutoComplete("query", "ydQuery", "aa", true, false, "", true, "http://www.youdao.com");
      aa.setSearchServer("http://www.youdao.com/search?");
      aa.setLogServer("http://www.youdao.com/");
      aa.setKeyFrom("163.index");
      aa.showContent = function(){
        var inputbox = document.getElementById("ydQuery");
        var position = SP.cumOffset(inputbox);
	if (getUserType()!=0) {
	this.sdiv.style.top = position[1] + (inputbox.offsetHeight - 1) + "px";
	} else {
        this.sdiv.style.top = position[1] + (inputbox.offsetHeight + 2) + "px";
	}
	
	if (getUserType()!=0) {
		if ((this.bName == "IE" && this.bVer == 8) || (navigator.userAgent.indexOf('Trident') >0 && (this.bName == "IE" && this.bVer!=6)  )) {
	          this.sdiv.style.left = (position[0] - 1) + "px";
	        } else {
		  this.sdiv.style.left = (position[0] ) + "px";
	        }
		this.sdiv.style.width = (inputbox.offsetWidth + 1) + "px";
	}  else {
		if ((this.bName == "IE" && this.bVer == 8) || (navigator.userAgent.indexOf('Trident') >0 && (this.bName == "IE" && this.bVer!=6)  )) {
                  this.sdiv.style.left = (position[0] - 2) + "px";
                } else {
                  this.sdiv.style.left = (position[0] - 1) + "px";
                } 
		this.sdiv.style.width = (inputbox.offsetWidth + 2) + "px";
	}
        this.sdiv.style.cursor = "default";
        SElement.show(this.sdiv);
        this.vis = true;
        this.curNodeIdx = -1;
      }
      SEvent.observe(acIcon, "click", aa.pressPoint.sbAEListener(aa));

      SEvent.observe(acIcon, "mouseover", aa.onmouseover2.sbAEListener(aa));
      SEvent.observe(acIcon, "mouseout", aa.onmouseout2.sbAEListener(aa));
      aa.start();
    });
    if (oldui){
    SEvent.observe(document,'click',closeHotWords);
    SEvent.observe(hotIconEl,'click',showAllHotWords);
    }
  }
}
